<!DOCTYPE html>
<html>
<%-include('./partials/head.ejs')%>
<body>
<%-include('./partials/nav.ejs')%>
<main>
	<article class="blogs content">
		<% if(blogs.length>0){ %>
		<% blogs.forEach(blog=>{ %>
		<a class="single" href="/blogs/<%=blog._id%>">
		<h3 class="title"><%= blog.title%></h3>
		<p class="snippets"><%= blog.snippet%></p>
		</a>
		<% }) %>
		<% } %>
	</article>
</main>
<%-include('./partials/footer.ejs')%>

</body>
</html>